
public class For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
			for(int i =5; i>0; i--) {
				System.out.println("예");
				
			}
			
			for(int i2=0; i2<2; i2++) {
				System.out.println("예"+i2);
				for(int j=0; j<3; j++) {
					System.out.println("아니오"+j);
			}
				
			}
				
		
		
	}

}
