
public class Yeon {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i =0;
		i*=3;
		System.out.println(i);
	    byte j=6;
		System.out.println(j+3);
		System.out.println(j-=3);
		System.out.println(j*=3);
		System.out.println(j/=3);
		System.out.println(j%=3);
		System.out.println(j<<=3);
		System.out.println(j>>=3);
		System.out.println(j>>>=3);
		System.out.println(j&=3);
		System.out.println(j^=3);
		System.out.println(j|=3);
	}

}
