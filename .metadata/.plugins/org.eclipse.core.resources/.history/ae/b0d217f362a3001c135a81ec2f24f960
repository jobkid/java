
public class Control1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=5, b=15;
		if(a<10) {
			System.out.println("a="+a);
		}
		else {
			System.out.println(a);
		}
		System.out.println("----------");
		if (b>10) {
			System.out.println("b="+b);
		}

	}

}
